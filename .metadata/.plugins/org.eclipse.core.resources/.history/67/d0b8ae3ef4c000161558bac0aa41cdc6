public class TestLoop {
	int haha = 2;
    public void foo() {
        for (int j = 0; j < 2; j++) {
            for (int i = 0; i < 5; i++) {
            	if(i> haha){
            		System.out.println(i);
                }else{
                	System.out.println(j);
                }
            }
        }
    }
}